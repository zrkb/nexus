{
    "name": "zrkb/nexus",
    "description": "Nexus is an Admin Panel based on Laravel Framework",
    "keywords": [ "nexus", "admin", "laravel", "dashboard", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Ayala",
            "email": "hi@felixaya.la",
            "homepage": "http://felixaya.la",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/zrkb/nexus",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "konekt/html": "^6.5",
        "spatie/laravel-activitylog": "^3.8|^4.0",
        "spatie/laravel-permission": "^3.0|^4.0|^5.0|^6.0",
        "plank/laravel-mediable": "^3.0|^4.0|^5.0|^6.0",
        "laravel/ui": "^1.0|^2.0|^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0|^10.0",
        "barryvdh/laravel-debugbar": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Nexus\\": "src"
        },
        "files": [
            "src/Helpers/nexus_helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Nexus\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nexus\\NexusServiceProvider"
            ],
            "aliases": {
                "Nexus": "Nexus\\Facades\\Nexus"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
