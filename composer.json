{
    "name": "imfx/nexus",
    "description": "Nexus is an Admin Panela based on Laravel Framework",
    "keywords": [ "nexus", "admin", "laravel", "dashboard", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Ayala",
            "email": "felix1262@gmail.com",
            "homepage": "http://felixaya.la",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/imfx/nexus",
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5.1",
        "spatie/laravel-html": "^2.19",
        "spatie/laravel-activitylog": "^3.1",
        "spatie/laravel-permission": "^2.9",
        "laravelcollective/html":"^5.8.0",
        "plank/laravel-mediable": "^2.9"
    },
    "require-dev": {
        "cybercog/laravel-whoops-editor": "^3.0",
        "phpunit/phpunit": ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Pandorga\\Nexus\\": "src"
        },
        "files": [
            "src/Helpers/nexus_helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Pandorga\\Nexus\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pandorga\\Nexus\\NexusServiceProvider"
            ],
            "aliases": {
                "Nexus": "Pandorga\\Nexus\\Facades\\Nexus"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
