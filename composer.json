{
    "name": "zrkb/nexus",
    "description": "Nexus is an Admin Panel based on Laravel Framework",
    "keywords": [ "nexus", "admin", "laravel", "dashboard", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Ayala",
            "email": "hi@felixaya.la",
            "homepage": "http://felixaya.la",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/zrkb/nexus",
    "require": {
        "php": "^7.2.5 | ^8.0",
        "illuminate/support": "^6.0 | ^7.0 | ^8.0",
        "spatie/laravel-html": "^2.23",
        "spatie/laravel-activitylog": "^3.8",
        "spatie/laravel-permission": "^3.0",
        "laravelcollective/html":"^6.0",
        "plank/laravel-mediable": "^3.0 | ^4.0",
        "laravel/ui": "^1.0 | ^2.0 | ^3.0"
    },
    "require-dev": {
        "cybercog/laravel-whoops-editor": "^3.0",
        "phpunit/phpunit": "^8.0 | ^9.0",
        "barryvdh/laravel-debugbar": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Nexus\\": "src"
        },
        "files": [
            "src/Helpers/nexus_helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Nexus\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nexus\\NexusServiceProvider"
            ],
            "aliases": {
                "Nexus": "Nexus\\Facades\\Nexus"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
